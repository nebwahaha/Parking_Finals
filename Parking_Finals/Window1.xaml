<Window x:Class="Parking_Finals.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parking_Finals"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <Button Content="Entrance booth" HorizontalAlignment="Left" VerticalAlignment="Top" Height="100" Width="200" Margin="150,175,0,0" Click="EntranceBoothButton_Click"/>
        <Button Content="Exit booth" HorizontalAlignment="Right" VerticalAlignment="Top" Height="100" Width="200" Margin="0,175,150,0" Click="ExitBoothButton_Click"/>
        <Button x:Name="AdminButton" Content="View Logs" HorizontalAlignment="Center" VerticalAlignment="Top" Height="100" Width="200" Margin="0,300,0,0" Click="AdminButton_Click" Visibility="Collapsed"/>
    </Grid>
</Window>
